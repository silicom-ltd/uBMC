#!/bin/bash

PKG_NAME=ttyd
PKG_VERSION=1.5.2
PKG_TAR_NAME=${PKG_NAME}-${PKG_VERSION}.tar.gz
PKG_DIR_NAME=${PKG_NAME}-${PKG_VERSION}

PATCH_LOCATION=${BR2_ROOT}/package/${PKG_NAME}
PKG_PRE_PATCH_DIR=${PATCH_LOCATION}

PATCH_NAME_MAIN=
PATCH_MAIN_EXCEPT_FILE=

PATCH_NAME[1]=0100-fix-ttyd-auth.patch
PATCH_FLIST[1]=files_ttyd_auth.txt
